{
  "name": "typescript-angular",
  "version": "1.0.0",
  "authors": [
    "Daniel Melo"
  ],
  "description": "AngularJS 1.x application with TypeScript 1.5",
  "main": "main.js",
  "license": "MIT",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "angular": "~1.4.4",
    "angular-route": "~1.4.4",
    "lodash": "~3.10.0",
    "qunit": "~1.18.0",
    "angular-typescript": "~0.0.3",
    "bootstrap": "~3.3.5",
    "angular-mocks": "~1.4.5",
    "sinon-js": "~1.15.4",
    "angular-loading-bar": "~0.8.0"
  },
  "resolutions": {
    "angular": "~1.4.4"
  },
  "overrides": {
    "angular": {
      "main": [
        "angular.js",
        "angular.min.js",
        "angular.min.js.map"
      ],
      "normalize": {
        "js": [
          "*.js",
          "*.map"
        ]
      }
    },
    "angular-route": {
      "main": [
        "angular-route.js",
        "angular-route.min.js",
        "angular-route.min.js.map"
      ],
      "normalize": {
        "js": [
          "*.js",
          "*.map"
        ]
      }
    },
    "angular-mocks": {
      "main": [
        "angular-mocks.js"
      ],
      "normalize": {
        "js": [
          "*.js"
        ]
      }
    },
    "lodash": {
      "main": [
        "lodash.min.js"
      ],
      "normalize": {
        "js": [
          "*.js",
          "*.map"
        ]
      }
    },
    "qunit": {
      "normalize": {
        "js": [
          "*.js",
          "*.map"
        ],
        "css": [
          "*.css"
        ]
      }
    },
    "angular-typescript": {
      "main": [
        "at-angular.ts"
      ],
      "normalize": {
        "ts": [
          "*.ts"
        ]
      }
    },
    "jquery": {
      "main": [
        "dist/jquery.min.js",
        "dist/jquery.min.map"
      ],
      "normalize": {
        "js": [
          "*.min.js",
          "*.min.map"
        ]
      }
    },
    "bootstrap": {
      "main": [
        "dist/js/*.min.js",
        "dist/fonts/*.*",
        "dist/css/*.min.css",
        "dist/css/*.css.map"
      ],
      "normalize": {
        "js": [
          "*.min.js"
        ],
        "fonts": [
          "*.woff",
          "*.woff2",
          "*.ttf",
          "*.svg",
          "*.eot"
        ],
        "css": [
          "*.min.css",
          "*.css.map"
        ]
      }
    },
    "sinon-js": {
      "main": [
        "sinon.js"
      ],
      "normalize": {
        "js": [
          "*.js"
        ]
      }
    },
    "angular-loading-bar" : {
      "main" : [
        "build/loading-bar.min.js",
        "build/loading-bar.min.css"
      ],
      "normalize" : {
        "js" : [
          "*.min.js"
        ],
        "css" : [
          "*.min.css"
        ]
      }
    }
  }
}
